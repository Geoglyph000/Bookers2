<h1>Users</h1>
<% @users.each do |user| %>
<div>
    <p class="user_image"><%= attachment_image_tag @user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></p>
    <p class="username"><%= user.name %></p>
    <%= link_to "show", user_path(user.id) %>
</div>
<% end %>
<%= paginate @users %>